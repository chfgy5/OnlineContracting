        <td><input id="@item.info_id-@item.carrier_name-FollowUp" type="date" value='@string.Format("{0:yyyy-MM-dd}", item.FollowUp)' onchange="saveDate(@item.info_id, value)"/></td>
        <td><input id="@item.info_id-@item.carrier_name-Note" type="text" value='@item.notes' onchange="saveNote(@item.info_id, value)"/></td>
    </tr>
