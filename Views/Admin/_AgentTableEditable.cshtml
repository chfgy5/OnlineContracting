@model IEnumerable<OnlineContracting.Models.info>
@Scripts.Render("~/bundles/jquery")

<script type="text/javascript">
    $(document).ready(addStyle());
</script>


@foreach (var item in Model)
{
    <tr>
        <td>@Html.DisplayFor(modelItem => item.carrier_name)</td>
        @* The ondblclick string works, but if you reverse the single and double quotes it breaks... *@
