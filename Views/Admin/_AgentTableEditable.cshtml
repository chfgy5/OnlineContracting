        <td id="@item.info_id-@item.carrier_name-NV" ondblclick='changeData("@item.info_id", "@item.carrier_name", "NV")'>@Html.DisplayFor(modelItem => item.NV)</td>
        <td id="@item.info_id-@item.carrier_name-OH" ondblclick='changeData("@item.info_id", "@item.carrier_name", "OH")'>@Html.DisplayFor(modelItem => item.OH)</td>
        <td id="@item.info_id-@item.carrier_name-OK" ondblclick='changeData("@item.info_id", "@item.carrier_name", "OK")'>@Html.DisplayFor(modelItem => item.OK)</td>
