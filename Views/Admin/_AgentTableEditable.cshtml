        <td id="@item.info_id-@item.carrier_name-GA" ondblclick='changeData("@item.info_id", "@item.carrier_name", "GA")'>@Html.DisplayFor(modelItem => item.GA)</td>
        <td id="@item.info_id-@item.carrier_name-ID" ondblclick='changeData("@item.info_id", "@item.carrier_name", "ID")'>@Html.DisplayFor(modelItem => item.ID)</td>
        <td id="@item.info_id-@item.carrier_name-IL" ondblclick='changeData("@item.info_id", "@item.carrier_name", "IL")'>@Html.DisplayFor(modelItem => item.IL)</td>
