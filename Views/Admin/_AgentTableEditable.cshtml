        <td id="@item.info_id-@item.carrier_name-KS" ondblclick='changeData("@item.info_id", "@item.carrier_name", "KS")'>@Html.DisplayFor(modelItem => item.KS)</td>
        <td id="@item.info_id-@item.carrier_name-MI" ondblclick='changeData("@item.info_id", "@item.carrier_name", "MI")'>@Html.DisplayFor(modelItem => item.MI)</td>
        <td id="@item.info_id-@item.carrier_name-MO" ondblclick='changeData("@item.info_id", "@item.carrier_name", "MO")'>@Html.DisplayFor(modelItem => item.MO)</td>
        <td id="@item.info_id-@item.carrier_name-MS" ondblclick='changeData("@item.info_id", "@item.carrier_name", "MS")'>@Html.DisplayFor(modelItem => item.MS)</td>
        <td id="@item.info_id-@item.carrier_name-NC" ondblclick='changeData("@item.info_id", "@item.carrier_name", "NC")'>@Html.DisplayFor(modelItem => item.NC)</td>
